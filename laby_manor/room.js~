	this.tileset = "tileset.png",
	this.evenements = function() {
		document.onkeydown = function(e) {
			switch(e.keyCode) {
				case 65://code pour la gauche
					if (directionX!=-1 && directionY==0) {dir = 0;}//Si on ne se dirige vers nul par, on oriente le perso
					directionX = -1;
				break;
				case 38://code pour le haut
					if (directionX==0 && directionY!=-1) {dir = 2;}//Si on ne se dirige vers nul par, on oriente le perso
					directionY = -1;
				break;
				case 69://code pour la droite
					if (directionX!=1 && directionY==0) {dir = 1;}//Si on ne se dirige vers nul par, on oriente le perso
					directionX = 1;
				break;
				case 40://code pour le bas
					if (directionX==0 && directionY!=1) {dir = 3;}//Si on ne se dirige vers nul par, on oriente le perso
					directionY = 1;
				break;
			}
		}

		document.onkeyup = function(e) {
			switch(e.keyCode) {
				case 65://quand on arrête d'appuyer sur la gauche
					if (directionY!=0) {//si on se dirige vers le bas ou le haut
						if (directionY==-1) {//si on se dirige vers le haut
							dir=2;//on retourne le bonhomme vers le haut
						} else { dir=3; }//sinon, on le retourne de nouveau vers le bas
					}
					directionX = 0;//on arrête de se déplacer horizontalement
				break;
				case 38://quand on arrête d'appuyer sur le haut
					if (directionX!=0) {//si on se dirige vers la droite ou la gauche
						if (directionX==-1) {//si on se dirige vers la gauche
							dir=0;//on redirige le bonhomme vers la gauche
						} else { dir=1; }//sinon, on redirige le bonhomme vers la droite
					}
					directionY = 0;//on arrête de se déplacer verticalement
				break;
				case 69://quand on arrête d'appuyer sur la droite
					if (directionY!=0) {//si on se dirige vers le bas ou le haut
						if (directionY==-1) {//si on se dirige vers le haut
							dir=2;//on redirige le bonhomme vers le haut
						} else { dir=3; }//sinon, on redirige le bonhomme vers la gauche
					}
					directionX = 0;//on arrête de se déplacer horizontalement
				break;
				case 40://quand on arrête d'appuyer sur le bas
					if (directionX!=0) {//si on se dirige vers la droite ou la gauche
						if (directionX==-1) {//si on se dirige vers la gauche
							dir=0;//on redirige le bonhomme vers la gauche
						} else { dir=1; }//sinon, on redirige le bonhomme vers la droite
					}
					directionY = 0;//on arrête de se déplacer verticalement
				break;
			}
		}
	},
		this.evenements2 = function() {
		if (posX>=423 && posX<=425 && posY>=30 && directionY==1){
			alert("Maintenant vous aller passer au niveau 2 de notre jeu");
		} 
	}
}
var map = new Map();
